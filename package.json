{
  "name": "dating-app",
  "version": "1.0.0",
  "description": "Dating app Packages",
  "main": "server.js",
  "scripts": {
    "build": "npm run build-clean && npm run build-sass && npm run build-autoprefix && npm run build-cleancss",
    "build-autoprefix": "postcss --use autoprefixer --map false --output app/static/css/main.css app/static/css/main.css",
    "build-clean": "rimraf css",
    "build-cleancss": "cleancss -o app/static/css/main.min.css app/static/css/main.css",
    "build-sass": "node-sass --output-style expanded --source-map true app/static/sass/main.sass app/static/css/main.css",
    "deploy": "npm run build",
    "test": "test",
    "start": "node server.js",
    "nmStart": "nodemon server.js",
    "sassStart": "npm run build-sass -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GiovanniDw/dating-app.git"
  },
  "keywords": [
    "Dating"
  ],
  "author": "Giovanni Dwidjosewojo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/GiovanniDw/dating-app/issues"
  },
  "homepage": "https://github.com/GiovanniDw/dating-app#readme",
  "dependencies": {
    "apicalypse": "^0.1.6",
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "ejs": "^3.0.1",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "mongoose": "^5.9.1",
    "multer": "^1.4.2",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^6.0.1",
    "passport-local-mongoose-email": "^0.1.4"
  },
  "devDependencies": {
    "ejs-lint": "^1.0.1",
    "nodemon": "^2.0.2",
    "autoprefixer": "^9.6.1",
    "clean-css-cli": "^4.3.0",
    "node-sass": "^4.12.0",
    
  }
}
